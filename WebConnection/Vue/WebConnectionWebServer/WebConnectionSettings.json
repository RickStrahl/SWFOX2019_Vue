{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Westwind.WebConnection":  "Warning" 
    }
  },
  "WebRoot": null,
  "Port": 5200,
  "UseSsl": "false",
  "UseLiveReload": true,
  "OpenBrowser": true,
  "ShowUrls": true,
  "DefaultFiles": "index.html, default.htm",
  "WebConnection": {
    "HandledExtensions": ".wc,.wcs,.wwd",
    "MessagingMechanism": "File",
    "ServerCount": 2
  },
  "AllowedHosts": "*"
}